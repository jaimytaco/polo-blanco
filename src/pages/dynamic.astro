---

import HomeLayout from '../layouts/HomeLayout.astro'
import { EDatabaseMode } from '../scripts/enums/database.enum'
import { Database } from '../scripts/modules/database'
import { Category } from '../scripts/models/category.model'
import { Router } from '../scripts/modules/router'

const viewId = 'public-dynamic'

await Router.initDatabase()
const { content, err } = await Router.getDynamicContent({ viewId })

---
    
<HomeLayout title="{ content.head.title }" meta="{ content.head.meta }">
    { content.body }
</HomeLayout>