---

import HomeLayout from '../layouts/HomeLayout.astro'
import { ViewGenerator } from '../scripts/modules/viewGenerator'

const viewId = 'public-dynamic'

await ViewGenerator.initDatabase()
const { content, err } = await ViewGenerator.getDynamicContent({ viewId })
if (err) throw err

---
    
<HomeLayout title="{ content.head.title }" meta="{ content.head.meta }">
    { content.body }
</HomeLayout>