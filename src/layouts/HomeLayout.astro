---

const { title, meta } = Astro.props

---

<!DOCTYPE html>
<html lang="es">
<head>
    <title>{title}</title>

    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    {meta}

    <link rel="stylesheet" type="text/css" href={Astro.resolve('../scripts/styles/utils.css')} />
</head>
<body>
    <slot/>
    <script defer type="module" src={Astro.resolve('../scripts/modules/main.js')}></script>
</body>
</html>